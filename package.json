{
  "name": "@formkit/tempo",
  "version": "0.0.8",
  "description": "The easiest way to work with dates in JavaScript and TypeScript.",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "cd ./docs && pnpm nuxt dev",
    "build": "tsup",
    "test": "TZ=\"America/New_York\" vitest",
    "docs-build": "cd ./docs && pnpm run build",
    "publint": "publint",
    "release": "pnpm build && bumpp && pnpm publish"
  },
  "files": [
    "dist",
    "docs/public"
  ],
  "keywords": [
    "date",
    "time",
    "internationalization",
    "date format"
  ],
  "author": "Justin Schroeder <justin@formkit.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.10",
    "bumpp": "^9.3.0",
    "publint": "^0.2.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  }
}
